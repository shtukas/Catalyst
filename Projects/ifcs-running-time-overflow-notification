#!/Users/pascal/.rvm/rubies/ruby-2.5.1/bin/ruby

# encoding: UTF-8

require_relative "Projects.rb"

# --------------------------------------------------------------------

exit

InFlightControlSystem::claims()
.each{|claim|
    uuid = claim["uuid"]
    next if !InFlightControlSystem::isRunning(uuid)
    # The item is running
    next if !InFlightControlSystem::runTimeInSecondsOrNull(uuid)
    next if InFlightControlSystem::runTimeInSecondsOrNull(uuid) < 3600
    CatalystCommon::onScreenNotification("In Flight Control System", "#{claim["description"]} is running for more than an hour")
    exit
}
